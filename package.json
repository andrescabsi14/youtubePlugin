{
  "name": "youtubePlugin",
  "version": "0.0.1",
  "supportSite": "http://learn.appdocumentation.com/plugin-tutorials/youtube-plugin-tutorial",
  "scripts": {
    "dev": "parcel ./control/content/index.html ./control/design/index.html ./control/settings/index.html ./widget/index.html -p 8080 --out-dir ./youtubePlugin_release/ && cp plugin.json youtubePlugin_release/",
    "buildd": "parcel build ./control/content/index.html ./control/design/index.html ./control/settings/index.html ./widget/index.html --out-dir ./youtubePlugin_release/ && cp plugin.json youtubePlugin_release/",
    "start": "npm run dev && npm run start:conf",
    "start:conf": "cp ./plugin.json youtubePlugin_release/",
    "build": "gulp build && cd .. && rm -rf youtubePlugin.zip && zip -r youtubePlugin.zip youtubePlugin_release/",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS"
  },
  "license": "ISC",
  "dependencies": {
    "@uirouter/angularjs": "1.0.20",
    "angular": "1.7.5",
    "angular-aria": "1.7.5",
    "angular-messages": "1.7.5",
    "angular-sanitize": "1.7.5",
    "bootstrap": "3.4.0",
    "bootswatch": "3.4.0",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "ng-tags-input": "3.2.0"
  },
  "devDependencies": {
    "angular-mocks": "1.7.5",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-plugin-ng-annotate": "0.3.2",
    "babel-plugin-syntax-decorators": "6.13.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "jest": "23.6.0",
    "node-sass": "4.11.0",
    "parcel-bundler": "1.10.3",
    "parcel-plugin-goodie-bag": "1.0.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "workbox-cli": "3.6.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11",
    "not ie <= 10"
  ]
}
